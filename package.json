    "@eventdex/leads": "portal:../eventdex-modules/src/modules/leads",
    "@eventdex/lounge": "portal:../eventdex-modules/src/modules/lounge",
    "@eventdex/manageappt": "portal:../eventdex-modules/src/modules/manageappt",
