        "@eventdex/manageappt": "0.0.177",
        "@eventdex/player": "0.1.383",
        "@eventdex/profile": "0.1.292",
        "@eventdex/salesforce": "0.1.32",
        "@eventdex/schedule": "0.1.495",
        "@eventdex/twilio": "0.1.17",
        "@eventdex/utils": "0.1.358",
        "@eventdex/video": "0.1.630",
        "@eventdex/widgets": "0.0.343",
        "@opentok/client": "^2.19.0",
        "@primer/octicons-react": "^9.4.0",
        "@types/jest": "^27.4.0",
        "@types/lodash.throttle": "^4.1.6",
        "@types/node": "^12.12.26",
        "@types/react": "^17.0.38",
        "@types/react-dom": "^17.0.11",
        "@types/react-router-dom": "^5.1.3",
        "axios": "0.21.4",
        "babel-plugin-direct-import": "^1.0.0",
        "circular-dependency-plugin": "^5.2.2",
        "concurrently": "^5.1.0",
        "cross-env": "^7.0.2",
        "express": "^4.17.1",
        "lodash.throttle": "^4.1.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-intl": "^5.24.4",
        "react-redux": "^7.2.2",
        "react-router-dom": "^5.1.2",
        "react-scripts": "^5.0.0",
        "redux-logger": "^3.0.6",
        "sass": "^1.55.0",
        "strip-color": "^0.1.0",
        "tss-react": "^4.9.0",
        "typescript": "^4.8.4"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.4.0",
        "@testing-library/react-hooks": "^3.2.1",
        "@types/enzyme": "^3.10.4",
        "@types/enzyme-adapter-react-16": "^1.0.5",
        "@types/moment-timezone": "^0.5.13",
        "customize-cra": "^1.0.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "enzyme-to-json": "^3.4.4",
        "husky": "^7.0.0",
        "inspectpack": "^4.5.2",
        "isomorphic-fetch": "^2.2.1",
        "jest-junit": "^9.0.0",
        "lint-staged": "^12.3.3",
        "mocha": "^6.2.2",
        "mocha-junit-reporter": "^1.23.3",
        "pixelmatch": "^5.1.0",
        "prettier": "^2.5.1",
        "react-app-rewired": "^2.1.11",
        "react-test-renderer": "^16.12.0",
        "start-server-and-test": "^1.10.8",
        "ts-jest": "^24.3.0",
        "util": "^0.12.4",
        "webpack-bundle-analyzer": "^4.5.0"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --write"
        ]
    },
    "scripts": {
        "startWithServer": "concurrently npm:server npm:dev",
        "dev": "react-scripts start",
        "build-old": "react-scripts build",
        "build-newer": "node ./scripts/build.js",
        "build": "react-app-rewired --max_old_space_size=18192 build",
        "amplifyBuild": "node ./node_modules/@eventdex/bootstrap/src/CI/amplifyBuild.js",
        "start": "react-app-rewired --max_old_space_size=18192 start",
        "test": "jest",
        "eject": "react-scripts eject",
        "lint": "eslint src/**/*.{ts,tsx}",
        "server": "node server.js",
        "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run --browser chrome",
        "cypress:ci": "cross-env CYPRESS_baseUrl=http://localhost:8081 start-server-and-test server http://localhost:8081 cypress:run",
        "deploy:twilio-cli": "cross-env REACT_APP_SET_AUTH=passcode npm run build && twilio rtc:apps:video:deploy --authentication=passcode --app-directory ./build",
        "modules:upgrade": "yarn up '@eventdex/*'",
        "modules:link": "node ./scripts/linkModules.js",
        "modules:unlink": "node ./scripts/linkModules.js -u",
        "prepare": "husky install"
    },
    "eslintConfig": {
        "extends": "react-app",
        "rules": {
            "no-shadow": "warn"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "proxy": "http://localhost:8081/",
    "jest": {
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ]
    },
    "packageManager": "yarn@3.1.1"
}
