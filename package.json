    "pixelmatch": "^5.1.0",
    "prettier": "^2.5.1",
    "react-app-rewired": "^2.1.11",
    "react-test-renderer": "^16.12.0",
    "start-server-and-test": "^1.10.8",
    "ts-jest": "^24.3.0",
    "util": "^0.12.4",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "startWithServer": "concurrently npm:server npm:dev",
    "dev": "react-scripts start",
    "build-old": "react-scripts build",
    "build-newer": "node ./scripts/build.js",
    "build": "react-app-rewired build",
    "amplifyBuild": "node ./node_modules/@eventdex/bootstrap/src/CI/amplifyBuild.js",
    "start": "react-app-rewired start",
    "test": "jest",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{ts,tsx}",
    "server": "node server.js",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "cypress:ci": "cross-env CYPRESS_baseUrl=http://localhost:8081 start-server-and-test server http://localhost:8081 cypress:run",
    "deploy:twilio-cli": "cross-env REACT_APP_SET_AUTH=passcode npm run build && twilio rtc:apps:video:deploy --authentication=passcode --app-directory ./build",
    "modules:upgrade": "yarn upgrade -L -S @eventdex",
    "modules:link": "node ./scripts/linkModules.js",
    "modules:unlink": "node ./scripts/linkModules.js -u",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-shadow": "warn"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:8081/",
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "packageManager": "yarn@3.1.1",
  "resolutions": {
    "eventdex-modules": "portal:../eventdex-modules",
    "@eventdex/api": "portal:../eventdex-modules/src/modules/api",
    "@eventdex/assets": "portal:../eventdex-modules/src/modules/assets",
    "@eventdex/auth": "portal:../eventdex-modules/src/modules/auth",
